       <section  class=" gradient-custom">
  <div class="container py-5 h-100">
    <div class="row justify-content-center align-items-center h-100">
      <div class="col-12 col-lg-9 col-xl-7">
        <div class="card shadow-2-strong card-registration" style="border-radius: 15px; margin-top: 42px;">
          <div class="card-body p-4 p-md-5">
            </form>
            <h3 class="mb-4 pb-2 pb-md-0 mb-md-5" style="text-align: center;">Add Product</h3>
            <form action="/admin/addPdt" enctype="multipart/form-data" method="post">

              <div class="row">
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input type="text" id="Pdtname" name="productname" class="form-control" placeholder="Product name" required />
                    
                  </div>

                </div>
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input type="text" id="price" name="price" class="form-control "  placeholder="Price" required>
                  </div>

                </div>
              </div>

               <div class="row">
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <select style=" color: #6c75a7;" name="category" class="select form-control" required>
                      <option  value="none"  selected  disabled hidden > Select a category</option>
                      {{#each result}}
                    <option value="{{this.categoryname}}">{{this.categoryname}}</option>
                      {{/each}}
                  </select>
                 
                  </div>

                </div>
                
              </div>
              <div class="row">
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input type="number" id="quandity" name="quandity" class="form-control"  placeholder="Quandity" required/>
                   
                  </div>

                </div>
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input type="text" id="brand" name="brand" class="form-control " placeholder="Brand" required />
                   
                  </div>

                </div>
                </div>

                <div class="row">
                <div class="col-md-12 mb-4">

                  <div class="form-outline">
                  <input type="text" id="discription" name="discription" class="form-control "  placeholder="Discription" required/>
                
                  </div>

                </div>
                </div>

                 <div class="row">
                <div class="col-md-12 mb-4">

                  <div class="form-outline">
                  <input type="text" id="discription" name="discription2" class="form-control "  placeholder="Discription" required/>
                
                  </div>

                </div>
                </div>


                <div class="row">
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input type="file" name="main_image" class="form-control-file" id="exampleFormControlFile1"  accept="image/png, image/jpeg, image/jpg">
                    <label class="form-label mb-3" for="firstName">Main image</label>
                  </div>
                  
                  <div class="form-outline">
                    <input type="file" name="sub_image" class="form-control-file " multiple id="exampleFormControlFile1" accept="image/png, image/jpeg, image/jpg">
                    <label class="form-label" for="firstName">Sub images</label>
                  </div>

                </div>
                </div>
           
              <div class="mt-4 pt-2" style="display: flex; justify-content: center;">
                <input class="btn btn-primary btn-lg" type="submit" value="Submit" />
              </div>

            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
